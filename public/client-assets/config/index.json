{
    "genericInfo": {
        "logo": {
            "type": "box", "children": [{
                "type": "image", "src": "logo.png", "width": 147, "alt": "Brand Logo"
            }]
        }
    },
    "routes": [
        {
            "path": "/", "descriptor": "home.json", "redirectTo": "/register",
            "screen": "Home", "layout": "SplitColumn"
        },
        {
            "path": "/register", "descriptor": "register.json",
            "screen": "Register", "layout": "SplitColumn"
        },
        {
            "path": "/setup", "descriptor": "acc-setup.json", "preloadDesc": true,
            "screen": "AccountSetup", "layout": "SplitColumn"
        },
        {
            "path": "/login", "descriptor": "login.json",
            "screen": "Login", "layout": "SingleColumn"
        },
        {
            "path": [
                "/dashboard",
                "/users/:userid?",
                "/users/add"
            ],
            "descriptor": "console/index.json",
            "screen": "Console", "layout": "Console",
            "subScreens": {
                "/users": {
                    "component": "UsersMain",
                    "descriptor": "console/users/index.json"
                },
                "/users/add": {
                    "component": "AddUser",
                    "descriptor": "console/users/add.json"
                },
                "/users/:userid": {
                    "component": "UserDetails",
                    "descriptor": "console/users/details.json"
                }
            }
        }
    ]
}