{
    "palette": {
        "text": {
            "primary": "#222222",
            "secondary": "#666666"
        },
        "custom": {
            "button": {
                "default": {
                    "bgColor": "#FFFFFF",
                    "bgColorHover": "#EEEEEE",
                    "borderColor": "#CCCCCC"
                },
                "primaryBtn": {
                    "color": "#FFFFFF",
                    "borderColor": "#E72F27",
                    "bgImage": "linear-gradient(90deg, #FF4130 22%, #E80B00 100%)"
                },
                "secondaryBtn": {
                    "bgColor": "#FFFFFF",
                    "bgColorHover": "#FFFFFF",
                    "borderColor": "#CCCCCC"
                },
                "socialSigningBtn": {
                    "bgColor": "#FFFFFF",
                    "bgColorHover": "#FFFFFF",
                    "shadow": "0 2px 16px rgba(0, 0, 0, 0.16)"
                },
                "toggleBtn": {
                    "borderColor": "#FF9191",
                    "bgColorActive": "#FFF1F1"
                }
            },
            "link": {
                "default": {
                    "color": "#2ea3f2"
                }
            },
            "layout": {
                "split": {
                    "leftCol": {
                        "bgColor": "#EEEEEE",
                        "title": {
                            "color": "#0057B0"
                        },
                        "content": {
                            "color": "#6B6A6A"
                        }
                    }
                },
                "console": {
                    "borderColor": "#D8D8D8",
                    "leftCol": {
                        "accordion": {
                            "summary": {
                                "color": "#555555"
                            },
                            "content": {
                                "color": "#222222",
                                "bgColorHover": "#EEEEEE",
                                "bgColorActive": "#CEE7FF"
                            }
                        }
                    }
                }
            }
        }
    }
}